{
  "presets": ["@babel/preset-env", "@babel/preset-react"],
  "env": {
    "development": {
      "plugins": [
        ["babel-plugin-styled-components", {
          "displayName": true,
          "fileName": false
        }]
      ]
    },
    "test": {
      "presets": [
        "@babel/preset-env",
        "@babel/preset-typescript",
        "@babel/preset-react"
      ],
      "plugins": [
        "transform-runtime",
        "transform-es2015-modules-commonjs"
      ]
    }
  }
}